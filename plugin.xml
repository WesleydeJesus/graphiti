<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="net.sf.graphiti.ui.editors.GraphEditor"
            contributorClass="net.sf.graphiti.ui.editors.GraphActionBarContributor"
            default="true"
            extensions="xml, xdf, xnl"
            icon="icons/graphiti.gif"
            id="net.sf.graphiti.ui.editors.GraphEditor"
            matchingStrategy="net.sf.graphiti.ui.editors.GraphEditorMatchingStrategy"
            name="Graphiti graph editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="net.sf.graphiti.ui.wizards.graph"
            name="Graph">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="net.sf.graphiti.ui.Startup"></startup>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="net.sf.graphiti.ui.perspectives.GraphitiPerspectiveFactory"
            fixed="false"
            icon="icons/graphiti.gif"
            id="net.sf.graphiti.ui.perspective"
            name="Graphiti">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="net.sf.graphiti.ui.views"
            name="Graphiti">
      </category>
      <view
            allowMultiple="false"
            category="net.sf.graphiti.ui.views"
            class="net.sf.graphiti.ui.views.VertexParametersView"
            icon="icons/graphiti.gif"
            id="net.sf.graphiti.ui.views.VertexParametersView"
            name="Vertex parameters"
            restorable="true">
      </view>
   </extension>

</plugin>
